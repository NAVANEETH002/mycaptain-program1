#include<iostream>
#include<conio.h>
using namespace std;

int main()
{
    int arr[10],i;
    int *p;
    cout<<"Enter your array elements :"<<endl;
    for(i=0;i<5;i++)
        {
            cout<<"\n"<<"\t";
            cin>>arr[i];
        }
    cout<<"Entered elements are : "<<endl;
    for(i=0;i<5;i++)
    {
        p=&arr[i];
        cout<<"\n"<<"\t"<<*p;
        *p++;
    }
    return 0;
}
